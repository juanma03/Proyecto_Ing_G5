<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), method: :delete ) do |f| %>
	<%= f.input :name, label:"Contraseña actual",hint: "Ingresar contraseña actual para eliminar cuenta", required: true %>
	<div class="form-actions">
    	<%= f.button :submit, "Eliminar Cuenta" %>
 	</div>
<% end %>


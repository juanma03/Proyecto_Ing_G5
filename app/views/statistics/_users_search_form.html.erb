<%= form_tag({:controller => "statistics", :action => "por_post"}, :method => "post") do %>
  <%= label_tag "Fecha Inicial ", "Fecha Inicial" %>
  <br />
  <%= select_date Date.today, prefix: :start %>
  <br />
  <br />
  <%= label_tag "Fecha Final ", "Fecha Final" %>
  <br />
  <%= select_date Date.today, prefix: :end %>
  <br />
  <br />
  <%= submit_tag "Buscar" %>
<% end %>